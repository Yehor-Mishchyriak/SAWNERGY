{
    "FramesAnalyzer":{
        "output_directory_name": "Processed_Frames_{time}",
        "cpptraj_file_name": "{start}-{end}.dat",
        "pairwise_electrostatic_analysis": "pairwise :* :* cuteelec 1.0",
        "cpptraj_output_type": "avgout"
    },
    "AnalysesProcessor":{
        "output_directory_name": "Raw_Electrostatics_{time}",
        "csv_file_header": "residue_i,residue_i_index,residue_j,residue_j_index,energy\n",
        "csv_file_name": "{start}-{end}.csv"
    },
    "ToMatricesConverter":{
        "output_directory_name": "Int_Prob_Matrices_{time}",
        "matrices_directory_name": "{start}-{end}",
        "interactions_matrix_name": "interactions.npy",
        "probabilities_matrix_name": "probabilities.npy",
        "id_to_res_map_name": "id_to_res_map.txt"
    },
    "Protein":{
        "output_directory_name": "Generated_pathways_{time}",
        "pathways_file_name": "pathways_{index}.apa",
        "pathways_file_header": "HEADER:\n  start_residue: {start_residue}\n    number_steps: {number_steps}\n  target_residues: {target_residues}\n    number_pathways: {number_pathways}\n    seed: {seed}\n"
    }
}